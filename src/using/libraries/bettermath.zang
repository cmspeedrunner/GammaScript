fn math_sum(num)
    return num * (num + 1) / 2
end

fn math_sqr(num)
   return num*num
end

fn math_sqrt(num)
   return num ^ 0.5

end

fn math_expn(num)
   return num ^ num
end

let countulist = []


fn math_countup(num)
   for i = 0 to num then
       append(countulist, i)
   end
   return countulist
end


   
